using System;

namespace Look_a_this
{
    class Program
    {
        static void Main(string[] args)
        {

            double diesel = 1.47;
            double unleaded = 2.06;
            double octane = 2.17;

            Console.WriteLine("Initial price : ");
            Console.WriteLine("diesel = " + diesel);
            Console.WriteLine("unleaded = " + unleaded);
            Console.WriteLine("octane = " + octane );

            diesel = 1.1 * diesel;
            unleaded = 1.1 * unleaded;
            octane = 1.1 * octane;
            Console.WriteLine("\nAfter price : ");
            Console.WriteLine("diesel = " + diesel);
            Console.WriteLine("unleaded = " + unleaded);
            Console.WriteLine("octane = " + octane);

            Console.ReadLine();
        }
      
}
    }





using System;
					
public class Program
{
	public static void Main()
	{
 
		Console.Write("How much money to buy lunch :");
		decimal money =  Convert.ToDecimal(Console.ReadLine()); 
		Console.Write("How many people sharing :");
		decimal amt =  Convert.ToDecimal(Console.ReadLine());
	     decimal result = money / amt;
		Console.WriteLine("$" + result)
}}
